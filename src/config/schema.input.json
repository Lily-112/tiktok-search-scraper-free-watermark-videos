{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TikTokSearchInput",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "keyword": { "type": "string", "minLength": 1 },
    "limit": { "type": "integer", "minimum": 1, "maximum": 500 },
    "sortType": { "type": "integer", "enum": [0, 1, 2], "description": "0=Relevance, 1=Most liked, 2=Most recent" },
    "region": { "type": "string", "pattern": "^[A-Z]{2}$" },
    "publishTime": { "type": "string" },
    "isDownloadVideo": { "type": "boolean" },
    "isDownloadVideoCover": { "type": "boolean" },
    "outputPath": { "type": "string" },
    "mock": { "type": "boolean" }
  },
  "required": ["keyword"]
}